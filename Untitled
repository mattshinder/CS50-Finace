CREATE TABLE "users" ("id" SERIAL PRIMARY KEY NOT NULL, "username" TEXT NOT NULL, "hash" TEXT NOT NULL, "cash" NUMERIC NOT NULL DEFAULT 10000.00 );
CREATE UNIQUE INDEX "username" ON "users" ("username");
CREATE TABLE "stocks" ("id" INTEGER PRIMARY KEY NOT NULL, "user_id" INTEGER, "symbol" TEXT, "price" INTEGER, "date_and_time" TIMESTAMP DEFAULT CURRENT_TIMESTAMP, "number" INTEGER);